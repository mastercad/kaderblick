{
    "name": "byte-artist/fussballmanager",
    "description": "Webapplication to manage football teams and players",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Kempe",
            "email": "andreas.kempe@kaderblick.de"
        }
    ],   
    "require": {
        "symfony/skeleton": "^7.2",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "symfony/security-bundle": "^7.2",
        "lexik/jwt-authentication-bundle": "^3.1",
        "symfony/console": "^7.2",
        "symfony/flex": "^2.7",
        "api-platform/doctrine-orm": "*",
        "api-platform/symfony": "*",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/asset": "^7.2",
        "symfony/expression-language": "^7.2",
        "phpstan/phpdoc-parser": "^2.1",
        "symfony/property-access": "^7.2",
        "symfony/property-info": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/yaml": "^7.2",
        "symfony/http-kernel": "^6.4",
        "symfony/http-foundation": "^7.2",
        "symfony/dependency-injection": "^6.4",
        "symfony/config": "^6.4",
        "symfony/mailer": "^7.2",
        "symfony/messenger": "^7.2",
        "symfony/doctrine-messenger": "^7.2",
        "symfony/form": "^7.2",
        "symfonycasts/reset-password-bundle": "^1.23",
        "league/flysystem-bundle": "^3.4",
        "masbug/flysystem-google-drive-ext": "^2.4",
        "google/apiclient": "^2.0",
        "symfony/monolog-bundle": "^3.10",
        "minishlink/web-push": "^9.0",
        "symfony/http-client": "^7.3",
        "symfony/dom-crawler": "^7.3",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "league/oauth2-google": "^4.0",
        "beberlei/doctrineextensions": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.63",
        "symfony/stopwatch": "^7.2",
        "symfony/web-profiler-bundle": "^7.2",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "fakerphp/faker": "^1.24",
        "phpunit/phpunit": "^12.2",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "liip/test-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.76",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.13",
        "chindit/phpcs-extra": "^0.2.0",
        "phpcsstandards/phpcsextra": "^1.4",
        "phpcsstandards/phpcsutils": "^1.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "vimeo/psalm": "^6.12",
        "symfony/doctrine-bridge": "^7.3"
    }
}
